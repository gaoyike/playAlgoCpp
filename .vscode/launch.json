{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "showLog": false,
    "showtrace": false,
    "console": "none",
    "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit"
    ],
    "configurations": [
        {
            "name": "debugCurrentFile",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/${relativeFileDirname}/${relativeFileDirname}.exe",
            "args": [],
            "stopAtEntry": false,
            "targetArchitecture": "x86",
            "internalConsoleOptions": "openOnSessionStart",
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "D:\\Ming\\bin\\gdb.exe",
            "miDebuggerArgs": "--silent",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "buildCurrentFile",
            "postDebugTask": "runCurrentFile",
            "logging": { 
                "moduleLoad": false, 
                "engineLogging": false, 
                "trace": false,
                "exceptions": false,
                "programOutput": false,
                "traceResponse": false
              }
        }
    ]
}